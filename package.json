{
  "name": "@geedium/nuxt-tscss",
  "description": "Use styles directly in templates",
  "version": "1.0.2",
  "main": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "yarn && babel src --out-dir dist --extensions .ts --copy-files && tsc -b",
